<div class="login-container">
	
	<div class="login-header login-caret">
		
		<div class="login-content">
			
			<a href="index.html" class="logo">
				<img src="<%=request.getContextPath()%>/resources/assets/images/ESSTHSES2.png" width="320" alt="" />
			</a>
			
			<p class="description">L'Ecole Superieure des Sciences et de Technologie de Hammam Sousse (ESSTHS)</p>
			
			<!-- progress bar indicator -->
			<div class="login-progressbar-indicator">
				<h3>43%</h3>
				<span>logging in...</span>
			</div>
		</div>
		
	</div>
	
	
		<div></div>
	</div>
	

	
		<br>
		<br>
<center>		<%
 
String errorString = (String)request.getAttribute("error");
if(errorString != null && errorString.trim().equals("true")){
out.println("Le Pseudo ou le  mot de passe entré est incorrect");
}
%></center>
			
	
		
		<div class="login-content">
			
			
			<form action="j_spring_security_check" method="post">
				
				<div class="form-group">
					
					<div class="input-group">
						<div class="input-group-addon">
							<i class="entypo-user"></i>
						</div>
						
						<input type="text" class="form-control" name="j_username" placeholder="Pseudo" autocomplete="off" />
					</div>
					
				</div>
				
				<div class="form-group">
					
					<div class="input-group">
						<div class="input-group-addon">
							<i class="entypo-key"></i>
						</div>
						
						<input type="password" class="form-control" name="j_password"  placeholder="Mot de passe " autocomplete="off" />
					</div>
				
				</div>
				
				<div class="form-group">
				
				<button type="submit" class="btn btn-primary btn-block btn-login">
						<i class="entypo-login"></i>
						Connexion
					</button> 
				</div>
				
				<!-- Implemented in v1.1.4 -->				<div class="form-group">
					
				</div>
				
				<div class="form-group">
				
					
					
				</div>
				
